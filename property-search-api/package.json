{
  "name": "property-search-api",
  "version": "1.0.0",
  "description": "Property search API with semantic search and AI chatbot",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest watch",
    "test:env": "vitest run src/config/env.test.ts",
    "test:db-optimization": "vitest run src/__tests__/database/",
    "lint": "eslint src/**/*.ts",
    "migrate": "node -r tsx/cjs src/scripts/migrate.ts",
    "setup-db-optimization": "tsx scripts/setup-database-optimization.ts",
    "setup-db-optimization:all": "tsx scripts/setup-database-optimization.ts --all",
    "setup-db-optimization:indexes": "tsx scripts/setup-database-optimization.ts --create-indexes",
    "setup-db-optimization:analyze": "tsx scripts/setup-database-optimization.ts --analyze",
    "setup-db-optimization:report": "tsx scripts/setup-database-optimization.ts --report",
    "test:performance": "tsx scripts/run-performance-tests.ts",
    "test:performance:database": "tsx scripts/run-performance-tests.ts --database",
    "test:performance:api": "tsx scripts/run-performance-tests.ts --api",
    "test:performance:pool": "tsx scripts/run-performance-tests.ts --pool",
    "test:performance:all": "tsx scripts/run-performance-tests.ts --all",
    "test:performance:help": "tsx scripts/run-performance-tests.ts --help"
  },
  "dependencies": {
    "axios": "^1.10.0",
    "bcryptjs": "^2.4.3",
    "cloudinary": "^1.41.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^4.18.0",
    "express-rate-limit": "^7.5.1",
    "helmet": "^7.1.0",
    "ioredis": "^5.6.1",
    "jsonwebtoken": "^9.0.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.0",
    "pg": "^8.11.0",
    "rate-limit-redis": "^4.2.1",
    "redis": "^4.6.0",
    "socket.io": "^4.7.0",
    "stripe": "^14.0.0",
    "winston": "^3.11.0",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@testcontainers/postgresql": "^11.0.3",
    "@types/bcryptjs": "^2.4.0",
    "@types/compression": "^1.7.0",
    "@types/cors": "^2.8.0",
    "@types/express": "^4.17.0",
    "@types/jest": "^29.5.0",
    "@types/jsonwebtoken": "^9.0.0",
    "@types/morgan": "^1.9.0",
    "@types/multer": "^1.4.0",
    "@types/nock": "^10.0.3",
    "@types/node": "^20.0.0",
    "@types/nodemailer": "^6.4.0",
    "@types/pg": "^8.10.0",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "eslint": "^8.0.0",
    "jest": "^29.7.0",
    "nock": "^14.0.5",
    "supertest": "^7.1.1",
    "ts-jest": "^29.4.0",
    "tsx": "^4.0.0",
    "typescript": "^5.0.0",
    "vitest": "^3.2.4"
  }
}